  {{#each quartos}}
        <div class="card mt-4" style="margin-bottom: 250px;">
            <div class="card-body">
                <h2>{{dataValues.id}}</h2>
                <h4>Número: {{dataValues.number}}</h4>
                <h4>Diária: {{dataValues.diaria}}</h4>
                <h4>Máximo de pessoas: {{dataValues.npessoas}}</h4>
                <h4>Data inicial: {{dataValues.inicio}}</h4>
                <h4>Data Final: {{dataValues.fim}}</h4>

                <a href="http://localhost:8081/admin/editRoom/{{dataValues.id}}">
                    <button class="btn btn-success">Editar</button>
                </a>
            </div>
        </div>
  {{/each}}

  
    
        

